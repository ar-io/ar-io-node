  "scripts": {
    "build": "yarn clean && npx tsc --project ./tsconfig.prod.json && yarn copy-files",
    "copy-files": "copyfiles -u 1 src/**/*.graphql src/**/*.sql src/lib/redis-lua/** dist",
    "clean": "npx rimraf [ .nyc_output coverage dist ]",
    "start": "node --no-deprecation --import ./register.js --env-file=.env src/app.ts",
    "start:prod": "yarn build && node dist/app.js",
    "watch": "nodemon",
    "db:migrate": "node --import ./register.js src/migrate.ts",
    "db:dump-test-schemas": "./test/dump-test-schemas",
    "test": "node --no-deprecation --import ./register.js --test --test-concurrency 1 --enable-source-maps src/*.test.ts src/**/*.test.ts",
    "test:ci": "npx c8 -r lcov node --no-deprecation --import ./register.js --test --test-concurrency 1 --test-reporter=spec --test-reporter-destination=stdout --enable-source-maps src/*.test.ts src/**/*.test.ts",
    "test:coverage": "npx c8 -r text -r html node --no-deprecation --import ./register.js --test --test-concurrency 1 --enable-source-maps src/*.test.ts src/**/*.test.ts",
    "test:e2e": "node --no-deprecation --import ./register.js --test --test-concurrency 1 --test-reporter=spec --test-reporter-destination=stdout --enable-source-maps test/*.test.ts test/**/*.test.ts",
    "test:file": "node --no-deprecation --import ./register.js --test --test-concurrency 1 --enable-source-maps",
    "test:file:coverage": "npx c8 -r text -r html node --no-deprecation --import ./register.js --test --test-concurrency 1 --enable-source-maps",
    "test:features": "cucumber-js",
    "lint:check": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "duplicate:check": "jscpd src",
    "duplicate:report": "jscpd src --reporters html,console",
    "duplicate:ci": "jscpd src --reporters console --exitCode 1",
    "deps:check": "madge --circular --warning src",
    "deps:graph": "madge --image reports/dependency-graph.svg src",
    "deps:orphans": "madge --orphans --exclude '\\.test\\.ts$' src",
    "deps:leaves": "madge --leaves --exclude '\\.test\\.ts$' src",
    "deps:summary": "madge --summary src",
    "deps:ci": "madge --circular --no-spinner --no-color src",
    "service:start": "scripts/service start",
    "service:stop": "scripts/service stop",
    "service:restart": "scripts/service restart",
    "service:status": "scripts/service status",
    "service:logs": "scripts/service logs"
  }
