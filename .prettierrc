{
  "semi": true,
  "printWidth": 80,
  "trailingComma": "all",
  "singleQuote": true,
  "tabWidth": 2,
  "importOrder": ["<THIRD_PARTY_MODULES>", "^[.{1,2}/]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "overrides": [
    {
      "files": "**/*.md",
      "options": {
        "proseWrap": "always"
      }
    }
  ]
}
